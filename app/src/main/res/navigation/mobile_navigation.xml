<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.farmahelp1.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_magazine"
        android:name="com.example.farmahelp1.ui.dashboard.DashboardFragment"
        android:label="@string/title_magazine"
        tools:layout="@layout/fragment_magazine" />

    <fragment
        android:id="@+id/navigation_timeTable"
        android:name="com.example.farmahelp1.ui.notifications.NotificationsFragment"
        android:label="@string/title_timeTable"
        tools:layout="@layout/fragment_time_table" />
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.farmahelp1.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>